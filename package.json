{
  "name": "@curder/npm-vue-demo",
  "description": "A demo for vue component.",
  "author": "curder <q.curder@gmail.com>",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/npm-vue-demo.js",
  "dependencies": {
    "core-js": "^2.6.5",
